{
    "name": "talha/privacybee",
    "description": "Your app description",
    "type": "project",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@php artisan test"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "symfony/test-pack": "^1.0"
    }
}
